<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="tool.kvdao.xodusInitializer" class="com.truward.kvdao.xodus.init.XodusInitializer">
    <property name="temp" value="${liten.tool.bm.kvdao.isTemp}"/>
    <property name="dirName" value="${liten.tool.bm.kvdao.dirName}"/>
  </bean>

  <bean id="tool.kvdao.xodusEnvironment" factory-bean="tool.kvdao.xodusInitializer" factory-method="getEnvironment"/>

  <bean id="tool.kvdao.iseCatalogDao" class="liten.catalog.dao.support.DefaultIseCatalogDao">
    <constructor-arg ref="tool.kvdao.xodusEnvironment"/>
  </bean>
</beans>
