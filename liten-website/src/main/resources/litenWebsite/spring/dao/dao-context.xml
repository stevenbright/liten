<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <import resource="datasource-context.xml"/>
  <import resource="tx-context.xml"/>

  <bean id="litenWebsite.dao.jdbcTemplate" class="org.springframework.jdbc.core.JdbcTemplate">
    <property name="dataSource" ref="litenWebsite.dao.dataSource"/>
  </bean>

  <bean class="liten.catalog.dao.support.DefaultCatalogDao" autowire="constructor"/>
</beans>
